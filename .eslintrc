{
  "extends": ["plugin:prettier/recommended"],

  "plugins": ["prettier"],

  "rules": {
    "prettier/prettier": "error"
  },

  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },

  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
